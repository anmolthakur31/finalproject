<div class="container bg-service">

<div class="row">
<div class="col-xs-12 col-sm-12 col-lg-12 col-md-12">
<% @service.each do |services|%>
<div class="col-md-3 col-sm-3 col-lg-3">
<div class="card card-deep-purple card-image bg-image sample-bg-image4">
<%= image_tag services.photo.url(), class: "img-responsive"%>
<div class="context color-orange">
<span class="text-title text-center"><%= link_to services.name, service_path(services)%></span>
</div>
</div>
</div>
<%end%>
</div>

</div>

</div>
