<style type="text/css">
    .icon{
        opacity: 0.7 !important;
    margin-left: auto;
    margin-right: auto;
    }
</style>
<div class="container" style="margin-top: 50px;">
  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 heading">
      <h2 style="margin-bottom: 50px;">Serve4Wheels</h2>
      
          <div class="col-md-12 col-sm-12 col-lg-12">
            <ul class="nav nav-tabs">
              <li class="active"><a data-toggle="tab" href="#home">Users</a>
            </li>
    <li><a data-toggle="tab" href="#menu1">Vehicles</a></li>
    <li><a data-toggle="tab" href="#menu2">Services</a></li>
    <li><a data-toggle="tab" href="#menu3">Bookings</a></li>
  </ul>

  <div class="tab-content">
    <div id="home" class="tab-pane fade in active">
      <h3>Users</h3>
        <div class="container" >
            <div class="row">
              <div class="card " >
            <table   class="table table-striped " >
                     <thead>
                        <tr>
                            <th>User Name</th>
                            <th>Booking Id</th>
                            <th>User vehicle reg no.</th>
                        </tr>
                    </thead>
                <tbody>
                <%User.all.each do |user|%>
                <tr>
                   <td><%=user.name%></td>
                   <td></td>
                   <td></td>
                   </tr>
                   <%end%>
                   </tbody>
            </table>
           </div>
        </div>
    </div>
    </div>
    <div id="menu1" class="tab-pane fade">
      <h3>Vehicles</h3>
       <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5></h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                            
                            <a class="close-link">
                                <i class="fa fa-times"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">
                          
           
            
           <table class="table table-striped table-bordered table-hover dataTables-example" >
                    <thead>
                    <tr>
                   
                        <th>Brand</th>
                        <th>Model</th>
                        <th>Variant</th>
                        <th>Start year</th>
                    </tr>
                    </thead>
                    <tbody>

            <%Vehicle.all.each do |vehicle| %>
                    <tr class="gradeX">
                        <td><%=vehicle.brand.name%>
                        </td>
                        <td><%=vehicle.model.name%></td>
                        <td class="center"><%=vehicle.variant.name%></td>
                        <td class="center"><%=vehicle.startyear%> </td>
                    </tr>
     <%end%>              
                    </tbody>
                    

                    </table>
  

                    </div>
                </div>
            </div>
            </div>
    </div>

    </div>
    <div id="menu2" class="tab-pane fade">
      <h3>Services</h3>
   
    </div>
    <div id="menu3" class="tab-pane fade">
      <h3>Bookings</h3>
     <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5></h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                            
                            <a class="close-link">
                                <i class="fa fa-times"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">
                          
           
            
           <table class="table table-striped table-bordered table-hover dataTables-example" >
                    <thead>
                    <tr>
                   
                        <th>Dateslot</th>
                        <th>Timeslot</th>
                        <th>User-Vehicle</th>
               
                    </tr>
                    </thead>
                    <tbody>

    <%Booking.all.each do |booking| %>
                    <tr class="gradeX">
                
                        <td><%=booking.dateslot%></td>
                        <td><%=booking.timeslot%></td>
                        <td><%=booking.uservehicle.reg_no%></td>
                  
                    </tr>
     <%end%>              
                    </tbody>
                    

                    </table>
  

                    </div>
                </div>
            </div>
            </div>
    </div>
    </div>

</div>
</div>
</div>
</div>
</div>
