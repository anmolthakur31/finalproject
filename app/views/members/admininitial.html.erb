    <div class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="panel-title"><h4>USERS</h4></div>
                    </div><!--.panel-heading-->
                    <div class="panel-body">

                        <div class="overflow-table">
                        <table class="display datatables-basic">
                        <!-- <table class="display datatables-basic"> gives tables view without search -->                         
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone Number</th>
                                    <th>Joining Date</th>
                                    <th>Update on</th>
                                    <th>User Role</th>
                                </tr>
                            </thead>

                            <tfoot>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone Number</th>
                                    <th>Joining Date</th>
                                    <th>Update on</th>
                                    <th>User Role</th>
                                </tr>
                            </tfoot>

                            <tbody>
                                <% @users.each do |user| %>
                                <tr>
                                    <td><%=user.name%></td>
                                    <td><%=user.email%></td>
                                    <td><%=user.phone%></td>
                                    <td><%=user.created_at%></td>
                                    <td><%=user.updated_at%></td>
                                    <td><%=user.userrole.rolename%></td>
                                </tr>
                                <%end%>
                            </tbody>
                        </table>
                        </div><!--.overflow-table-->

                    </div><!--.panel-body-->
                </div><!--.panel-->
            </div><!--.col-md-12-->
        </div><!--.row-->
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="panel-title"><h4>Vehicles</h4></div>
                    </div><!--.panel-heading-->
                    <div class="panel-body">

                        <div class="overflow-table">
                        <!-- <table class="display datatables-alphabet-sorting"> -->
                        <table class="display datatables-basic">
                        <!-- <table class="display datatables-basic"> gives tables view without search -->                         
                            <thead>
                                <tr>
                                    <th>Brand</th>
                                    <th>Model</th>
                                    <th>Variant</th>
                                    <th>Starting Year</th>
                                    <th>Created On</th>
                                    <th>Updated on</th>
                                </tr>
                            </thead>

                            <tfoot>
                                <tr>
                                    <th>Brand</th>
                                    <th>Model</th>
                                    <th>Variant</th>
                                    <th>Starting Year</th>
                                    <th>Created On</th>
                                    <th>Updated on</th>
                                </tr>
                            </tfoot>

                            <tbody>
                                <% @vehicles.each do |vehicle| %>
                                <tr>
                                    <td><%=vehicle.brand.name%></td>
                                    <td><%=vehicle.model.name%></td>
                                    <td><%=vehicle.variant.name%></td>
                                    <td><%=vehicle.startyear%></td>
                                    <td><%=vehicle.created_at%></td>
                                    <td><%=vehicle.updated_at%></td>
                                </tr>
                                <%end%>
                            </tbody>
                        </table>
                        </div><!--.overflow-table-->

                    </div><!--.panel-body-->
                </div><!--.panel-->
            </div><!--.col-md-12-->
        </div><!--.row-->

    </div><!--.content-->

 

 

