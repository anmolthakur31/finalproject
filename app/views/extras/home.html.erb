									    <%= f.text_field :name, class: "form-control",id: "name",placeholder: "Enter your Name" %>

								
								</div>
							</div>
						
	<br>
						
							<div class="cols-sm-10">
								<div class="input-group">
									<span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>

